<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>


  <context-param>
    <param-name>c</param-name>
    <param-value>c.value</param-value>
  </context-param>

  <context-param>
    <param-name>d</param-name>
    <param-value>d.value</param-value>
  </context-param>




<!--  <display-name>Archetype Created Web Application</display-name>-->

  <servlet>
    <servlet-name>myServlet</servlet-name>
    <servlet-class>com.qf.controller.MyServlet</servlet-class>
<!--    <load-on-startup>1</load-on-startup>-->

    <init-param>
      <param-name>a</param-name>
      <param-value>a.value</param-value>
    </init-param>

    <init-param>
      <param-name>b</param-name>
      <param-value>b.value</param-value>
    </init-param>
  </servlet>

<servlet-mapping>
  <servlet-name>myServlet</servlet-name>
  <url-pattern>/myServlet</url-pattern>
</servlet-mapping>

</web-app>
